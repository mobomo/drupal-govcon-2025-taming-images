{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "license": "GPL-2.0-or-later",
    "type": "project",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "chat": "https://www.drupal.org/node/314178",
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html"
    },
    "require": {
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^2.0@beta",
        "drupal/admin_toolbar": "^3.6.2",
        "drupal/ai_image_alt_text": "^1.0.1",
        "drupal/bg_image_formatter": "^1.18",
        "drupal/core-composer-scaffold": "^11.2.3",
        "drupal/core-project-message": "^11.2.3",
        "drupal/core-recipe-unpack": "^11.2.3",
        "drupal/core-recommended": "^11.2.3",
        "drupal/devel": "^5.4",
        "drupal/dropzonejs": "^2.11",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/filefield_paths": "^1.0@beta",
        "drupal/focal_point": "^2.1.2",
        "drupal/gemini_provider": "^1.0@beta",
        "drupal/gin": "^5.0.3",
        "drupal/image_effects": "^4.1@beta",
        "drupal/image_style_quality": "^1.7",
        "drupal/image_style_warmer": "^1.3@RC",
        "drupal/image_styles_mapping": "^1.8",
        "drupal/image_widget_crop": "^3.0",
        "drupal/imagemagick": "^4.0",
        "drupal/material_base": "^3.0",
        "drupal/media_alias_display": "^2.1.1",
        "drupal/media_bulk_upload": "^3.0.4",
        "drupal/media_duplicates": "^2.0.3",
        "drupal/media_entity_browser": "^3.0",
        "drupal/normalized_image_styles": "^2.1",
        "drupal/pathauto": "^1.13",
        "drupal/redirect": "^1.11",
        "drupal/schemadotorg": "^1.0@alpha",
        "drupal/stage_file_proxy": "^3.1.5",
        "drupal/svg_image_field": "^2.3.4",
        "drupal/views_bulk_edit": "^3.0",
        "drupal/webp": "^1.0@RC",
        "drush/drush": "^13.6.2",
        "enyo/dropzone": "^6.0@beta"
    },
    "require-dev": {
        "drupal/core-dev": "^11.2.3",
        "ergebnis/composer-normalize": "^2.47"
    },
    "replace": {
        "drupal/ctools": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "type": "drupal-library",
                "version": "6.0.0-beta.2",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/dropzone/dropzone/releases/download/v6.0.0-beta.2/dist.zip"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-recipe-unpack": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "php-tuf/composer-integration": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "recipes/{$name}": [
                "type:drupal-recipe"
            ]
        },
        "patches": {}
    }
}
